let raceNumber = Math.floor(Math.random() * 1000)

let registrationTime = 'early'

let runnerAge = 30

if (registrationTime === 'early' && runnerAge > 18) {
    console.log(raceNumber += 1000);
}

if (registrationTime === 'early' && runnerAge > 18) {
    console.log(`"your race time is at 9:30 and your race number is ${raceNumber}"`);
} else if (registrationTime !== 'early' && runnerAge > 18) {
    console.log(`"your race time is at 11:00 and your race number is ${raceNumber}`);
}

if (runnerAge <18) {
    console.log(`"your race time is at 12:30 and your race number is ${raceNumber}`)
}
